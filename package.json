{
  "name": "tenzor-lab",
  "description": "",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest",
    "prettify": "prettier . --write",
    "lint-staged": "lint-staged",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --open --config webpack.dev.js",
    "pretify": "npx prettier . --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "test/.*\\.(test|spec)\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/mocks/styleMock.ts",
      "\\.(png|jpg|svg)$": "<rootDir>/test/mocks/fileMock.ts",
      "^components/(.*)$": "<rootDir>/src/script/components/$1",
      "^api/(.*)$": "<rootDir>/src/script/api/$1",
      "^features/(.*)$": "<rootDir>/src/script/features/$1",
      "^assets/(.*)$": "<rootDir>/src/assets/$1",
      "^themes/(.*)$": "<rootDir>/src/script/app/styles/themes/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^style/(.*)$": "<rootDir>/src/style/$1",
      "^utils/(.*)$": "<rootDir>/src/script/utils/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/prepareTestEnvironment.ts"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "npx prettier --write"
    ],
    "**/*.scss": [
      "npx prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.9.1",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^9.8.6",
    "babel-plugin-styled-components": "^1.13.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^11.0.0",
    "msw": "^0.33.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "prettier": "2.3.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "styled-components": "^5.3.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@codegena/oapi3ts": "^3.0.0-alpha.3",
    "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
    "@dtsgenerator/decorate-typename": "^1.1.1",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/codemod": "^5.0.1",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@react-hook/mouse-position": "^4.1.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@svgr/webpack": "^5.5.0",
    "@types/dom-mediacapture-record": "^1.0.10",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/validator": "^13.6.3",
    "animate.css": "^4.1.1",
    "buffer": "^5.7.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "dtsgenerator": "^3.12.1",
    "get-user-locale": "^1.4.0",
    "html-react-parser": "^1.2.7",
    "jest-styled-components": "^7.0.5",
    "libphonenumber-js": "^1.9.23",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.0.2",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.0.0"
  }
}